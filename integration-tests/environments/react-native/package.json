{
  "name": "realmreactnativetests",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "postinstall": "install-local && npm run pod-install",
    "pod-install": "cd ios && pod install",
    "test": "npm run test/android && npm run test/ios",
    "test/ios": "mocha-remote --id ios -- concurrently npm:app-importer npm:metro npm:runner/ios",
    "test/android": "mocha-remote --id android -- concurrently npm:app-importer npm:metro npm:runner/android",
    "watch/ios": "mocha-remote --watch --id ios -- concurrently npm:app-importer npm:metro npm:runner/ios",
    "watch/android": "mocha-remote --watch --id android -- concurrently npm:app-importer npm:metro npm:runner/android",
    "app-importer": "realm-app-importer serve ../../realm-apps",
    "metro": "react-native start --reset-cache",
    "runner/android": "node harness/runner.js android",
    "runner/ios": "node harness/runner.js ios",
    "lint": "eslint ."
  },
  "dependencies": {
    "@react-native-community/art": "^1.2.0",
    "mocha": "^8.3.2",
    "mocha-junit-reporter": "^2.0.0",
    "mocha-remote-client": "^1.0.0",
    "path-browserify": "^1.0.1",
    "react": "17.0.1",
    "react-native": "0.64.0",
    "react-native-fs": "^2.16.6",
    "react-native-progress": "^4.1.2"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "babel-jest": "^26.6.3",
    "cmake-js": "file:/../../../node_modules/cmake-js",
    "concurrently": "^6.0.2",
    "eslint": "7.14.0",
    "fs-extra": "^9.1.0",
    "install-local": "^3.0.1",
    "metro-react-native-babel-preset": "^0.64.0",
    "mocha-remote-cli": "^1.0.0",
    "react-test-renderer": "17.0.1",
    "realm-app-importer": "file:../../../packages/realm-app-importer"
  },
  "localDependencies": {
    "realm": "../../..",
    "realm-integration-tests": "../../tests"
  }
}
