{
  "name": "realm-react-native-tests",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "react-native start",
    "prepare": "install-local && npm run pod-install",
    "pod-install": "cd ios && pod install",
    "test": "npm run test/android && npm run test/ios",
    "test/ios": "PLATFORM=ios WATCH=false npm run common",
    "test/android": "PLATFORM=android WATCH=false npm run common",
    "watch/ios": "PLATFORM=ios WATCH=true npm run common",
    "watch/android": "PLATFORM=android WATCH=true npm run common",
    "common": "mocha-remote --id $PLATFORM --watch $WATCH -- concurrently npm:app-importer npm:metro npm:runner",
    "app-importer": "realm-app-importer serve ../../realm-apps",
    "metro": "react-native start --reset-cache",
    "runner": "node harness/runner.js",
    "lint": "eslint ."
  },
  "dependencies": {
    "@react-native-community/art": "^1.2.0",
    "mocha": "^8.3.2",
    "mocha-junit-reporter": "^2.0.0",
    "mocha-remote-client": "^1.0.0",
    "path-browserify": "^1.0.1",
    "react": "17.0.1",
    "react-native": "0.64.0",
    "react-native-fs": "^2.16.6",
    "react-native-progress": "^4.1.2"
  },
  "peerDependencies": {
    "realm": "*",
    "realm-integration-tests": "*"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "babel-jest": "^26.6.3",
    "concurrently": "^6.0.2",
    "eslint": "7.14.0",
    "fs-extra": "^9.1.0",
    "install-local": "^3.0.1",
    "metro-react-native-babel-preset": "^0.64.0",
    "mocha-remote-cli": "^1.1.0",
    "react-test-renderer": "17.0.1",
    "realm-app-importer": "*"
  },
  "localDependencies": {
    "realm": "../../..",
    "realm-integration-tests": "../../tests"
  }
}
